<bindings
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns="http://java.sun.com/xml/ns/jaxb"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        version="2.1">

    <bindings schemaLocation="psicquic-registry.xsd" node="/xs:schema">
        <schemaBindings>
            <package name="org.hupo.psi.mi.psicquic.registry"/>
        </schemaBindings>
    </bindings>

    <globalBindings>
        <xjc:simple/>
    </globalBindings>

</bindings>
